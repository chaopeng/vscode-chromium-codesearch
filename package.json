{
    "name": "chromium-codesearch",
    "displayName": "chromium-codesearch",
    "description": "This extension is a vscode extension integate with chromiumcode search.",
    "version": "0.0.3",
    "publisher": "chaopeng",
    "repository": {
        "type": "git",
        "url": "https://github.com/chaopeng/vscode-chromium-codesearch"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:cpp",
        "onCommand:cs.refs",
        "onCommand:cs.open"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "cs.refs",
                "title": "CodeSearchReferences"
            },
            {
                "command": "cs.open",
                "title": "CodeSearchOpen"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "lru-cache": "^4.0.2",
        "request-promise-native": "^1.0.3",
        "spawn-command": "0.0.2-1"
    }
}